{
    "extends": "airbnb-base",
    "parserOptions": {
    	"ecmaVersion": 2017
    },
    "env": {
    	"node": true,
    	"browser": true,
    	"es6": true
    },
    "rules": {
        "comma-dangle": "off",
        "no-tabs": "off",
        "no-lonely-if": "off",
    	"indent": 0,
    	"no-console": 0,
        "object-shorthand": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "func-names": 0,
        "consistent-return": 0,
        "no-shadow": 0,
        "no-else-return": 0
    }
}