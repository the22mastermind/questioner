{
    "extends": "airbnb-base",
    // "extends": "eslint:recommended",
    "parserOptions": {
    	"ecmaVersion": 6
    },
    "env": {
    	"node": true,
    	"browser": true,
    	"es6": true
    },
    "rules": {
        "comma-dangle": "off",
        "no-tabs": "off",
        "no-lonely-if": "off",
    	"indent": 0,
    	"no-console": 0,
        "object-shorthand": 0,
        "no-undef": 0,
        "no-unused-vars": 0
    }
}